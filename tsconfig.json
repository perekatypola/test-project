{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react",
      "lib": ["es6"],
      "moduleResolution": "node",
      "noEmit": true,
      "module": "commonjs",
      "strict": true,
      "target": "esnext",
      "baseUrl": "./",
        "paths": {
        "*": ["src/*"] ,
        "Constants" : ["./src/Constants"],
        "Screens" : ["./src/Screens"],
        "Navigation" : ["./src/Navigation"]
    },
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
}